import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();   // size of array
        int sum = 0;

        for(int i = 0; i < n; i++) {
            int num = sc.nextInt();
            
            if(num % 2 == 0) {   // check even
                sum += num;
            }
        }

        System.out.println(sum);
    }
}
