import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        
        int n = s.nextInt(); // Read size of array
        int[] arr = new int[n];
        int sum = 0;
        
        for (int i = 0; i < n; i++) {
            arr[i] = s.nextInt(); // Read each element
            sum += arr[i]; // Add to sum
        }
        
        System.out.println(sum); // Print ONLY the sum
    }
}
