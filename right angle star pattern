import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int N = sc.nextInt();
        
        for (int i = 1; i <= N; i++) {        // Outer loop for rows
            for (int j = 1; j <= i; j++) {    // Inner loop for stars
                System.out.print("*");
            }
            System.out.println();             // Move to next line
        }
        
        sc.close();
    }
}
